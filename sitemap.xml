<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{{ range .Site.Pages }}
  <url>
    <loc>{{ .Permalink }}</loc>
    <lastmod>{{ safeHTML ( .Date.Format "2006-01-02" ) }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.5</priority>
  </url>
{{ end }}
</urlset>
